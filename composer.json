    "authors": [
        {
            "name" : "Product Advertising API Team",
            "homepage": "https://webservices.amazon.com/paapi5/documentation/index.html"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2||^7"
    },
    "require-dev": {},
    "config": {
        "optimize-autoloader": true
